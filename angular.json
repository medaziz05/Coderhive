{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "piwissal": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/piwissal",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/assets/css/vendor/bootstrap.min.css",
              "src/assets/css/vendor/animate.min.css",
              "src/assets/css/plugins/swiper.min.css",
              "src/assets/css/vendor/magnific-popup.css",
              "src/assets/css/vendor/fontawesome-pro.css",
              "src/assets/css/plugins/nouislider.min.css",
              "src/assets/css/vendor/icomoon.css",
              "src/assets/css/vendor/spacing.css",
              "src/assets/css/plugins/apexcharts.css",
              "src/assets/css/main.css",
              "src/styles.css"
              

            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/vendor/jquery-3.7.1.min.js",
              "src/assets/js/plugins/waypoints.min.js",
              "src/assets/js/vendor/bootstrap.bundle.min.js",
              "src/assets/js/plugins/swiper.min.js",
              "src/assets/js/plugins/wow.js",
              "src/assets/js/vendor/magnific-popup.min.js",
              "src/assets/js/vendor/isotope.pkgd.min.js",
              "src/assets/js/vendor/imagesloaded.pkgd.min.js",
              "src/assets/js/plugins/nice-select.min.js",
              "src/assets/js/vendor/ajax-form.js",
              "src/assets/js/plugins/easypie.js",
              "src/assets/js/vendor/purecounter.js",
              "src/assets/js/plugins/nouislider.min.js",
              "src/assets/js/plugins/apexcharts.min.js",
              "src/assets/js/plugins/typed.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "piwissal:build:production"
            },
            "development": {
              "browserTarget": "piwissal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "piwissal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "ba014c9a-b44e-4f9e-9f8d-2de319dac228"
  }
}
